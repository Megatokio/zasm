              	#!/usr/local/bin/zasm --reqcolon -o original/
              	;==============================================================
              	; WLA-DX banking setup
              	;==============================================================
              	.memorymap
              	defaultslot 0
              	slotsize $8000
              	slot 0 $0000
              	.endme
              	
              	.rombankmap
              	bankstotal 1
              	banksize $8000
              	banks 1
              	.endro
              	
              	;==============================================================
              	; SDSC tag and SMS rom header
              	;==============================================================
              	.sdsctag 1.2,"Hello World!","SMS programming tutorial program","Maxim"
              	
              	.bank 0 slot 0
0000:         	.org $0000
              	;==============================================================
              	; Boot section
              	;==============================================================
0000: F3      	    di              ; disable interrupts
0001: ED56    	    im 1            ; Interrupt mode 1
0003: C36800  	    jp main         ; jump to main program
              	
0006: FFFFFFFF	.org $0066
000A: FF...   	
              	;==============================================================
              	; Pause button handler
              	;==============================================================
              	    ; Do nothing
0066: ED45    	    retn
              	
              	;==============================================================
              	; Main program
              	;==============================================================
0068:         	main:
0068: 31F0DF  	    ld sp, $dff0
              	
              	    ;==============================================================
              	    ; Set up VDP registers
              	    ;==============================================================
006B: 21EF00  	    ld hl,VdpData
006E: 0612    	    ld b,VdpDataEnd-VdpData
0070: 0EBF    	    ld c,$bf
0072: EDB3    	    otir
              	
              	    ;==============================================================
              	    ; Clear VRAM
              	    ;==============================================================
              	    ; 1. Set VRAM write address to 0 by outputting $4000 ORed with $0000
0074: 3E00    	    ld a,$00
0076: D3BF    	    out ($bf),a
0078: 3E40    	    ld a,$40
007A: D3BF    	    out ($bf),a
              	    ; 2. Output 16KB of zeroes
007C: 010040  	    ld bc, $4000    ; Counter for 16KB of VRAM
007F:         	    ClearVRAMLoop:
007F: 3E00    	        ld a,$00    ; Value to write
0081: D3BE    	        out ($be),a ; Output to VRAM address, which is auto-incremented after each write
0083: 0B      	        dec bc
0084: 78      	        ld a,b
0085: B1      	        or c
0086: C27F00  	        jp nz,ClearVRAMLoop
              	
              	    ;==============================================================
              	    ; Load palette
              	    ;==============================================================
              	    ; 1. Set VRAM write address to CRAM (palette) address 0 (for palette index 0)
              	    ; by outputting $c000 ORed with $0000
0089: 3E00    	    ld a,$00
008B: D3BF    	    out ($bf),a
008D: 3EC0    	    ld a,$c0
008F: D3BF    	    out ($bf),a
              	    ; 2. Output colour data
0091: 21ED00  	    ld hl,PaletteData
0094: 0602    	    ld b,+(PaletteDataEnd-PaletteData)
0096: 0EBE    	    ld c,$be
0098: EDB3    	    otir
              	
              	    ;==============================================================
              	    ; Load tiles (font)
              	    ;==============================================================
              	    ; 1. Set VRAM write address to tile index 0
              	    ; by outputting $4000 ORed with $0000
009A: 3E00    	    ld a,$00
009C: D3BF    	    out ($bf),a
009E: 3E40    	    ld a,$40
00A0: D3BF    	    out ($bf),a
              	    ; 2. Output tile data
00A2: 210101  	    ld hl,FontData              ; Location of tile data
00A5: 01E00B  	    ld bc,FontDataEnd-FontData  ; Counter for number of bytes to write
00A8:         	    WriteTilesLoop:
              	        ; Output data byte then three zeroes, because our tile data is 1 bit
              	        ; and must be increased to 4 bit
00A8: 7E      	        ld a,(hl)        ; Get data byte
00A9: D3BE    	        out ($be),a
00AB: 23      	        inc hl           ; Add one to hl so it points to the next data byte
00AC: 0B      	        dec bc
00AD: 78      	        ld a,b
00AE: B1      	        or c
00AF: C2A800  	        jp nz,WriteTilesLoop
              	
              	    ;==============================================================
              	    ; Write text to name table
              	    ;==============================================================
              	    ; 1. Set VRAM write address to name table index 0
              	    ; by outputting $4000 ORed with $3800+0
00B2: 3E00    	    ld a,$00
00B4: D3BF    	    out ($bf),a
00B6: 3E78    	    ld a,$38|$40
00B8: D3BF    	    out ($bf),a
              	    ; 2. Output tilemap data
00BA: 21D500  	    ld hl,Message
00BD: 011800  	    ld bc,MessageEnd-Message  ; Counter for number of bytes to write
00C0:         	    WriteTextLoop:
00C0: 7E      	        ld a,(hl)    ; Get data byte
00C1: D3BE    	        out ($be),a
00C3: 23      	        inc hl       ; Point to next letter
00C4: 0B      	        dec bc
00C5: 78      	        ld a,b
00C6: B1      	        or c
00C7: C2C000  	        jp nz,WriteTextLoop
              	
              	    ; Turn screen on
00CA: 3EC0    	    ld a,%11000000
              	;          |||| |`- Zoomed sprites -> 16x16 pixels
              	;          |||| `-- Doubled sprites -> 2 tiles per sprite, 8x16
              	;          |||`---- 30 row/240 line mode
              	;          ||`----- 28 row/224 line mode
              	;          |`------ VBlank interrupts
              	;          `------- Enable display
00CC: D3BF    	    out ($bf),a
00CE: 3E81    	    ld a,$81
00D0: D3BF    	    out ($bf),a
              	
              	    ; Infinite loop to stop program
00D2:         	Loop:
00D2: C3D200  	     jp Loop
              	
